<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CompoundLayout">
        <attr name="checked" format="boolean" />
    </declare-styleable>
    <declare-styleable name="RadioLayoutGroup">
        <attr name="checkedButton" format="reference" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="GradientRadioLayout">
        <attr name="angle" format="integer" />
        <attr name="colorA" format="color" />
        <attr name="colorB" format="color" />
    </declare-styleable>
</resources>